{
  "components/common/header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n\r\n<header class=\"header{{#if theme_settings.haloStickyHeader}} header-sticky{{/if}}\">\r\n    {{#if theme_settings.halo_top_bar_promotion}}\r\n        <div class=\"halo-top-bar-promotion\">\r\n            <div class=\"container\">\r\n                {{#if theme_settings.halo-top_bar_promotion-link}}\r\n                    <a class=\"text\" href=\"{{theme_settings.halo-top_bar_promotion-link}}\">\r\n                        {{theme_settings.halo-top_bar_promotion-text}}\r\n                        <span>{{theme_settings.halo-top_bar_promotion-textLink}}</span>\r\n                    </a>\r\n                {{else}}\r\n                    <p class=\"text\">\r\n                        {{theme_settings.halo-top_bar_promotion-text}}\r\n                        <span>{{theme_settings.halo-top_bar_promotion-textLink}}</span>\r\n                    </p>\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n    <div class=\"halo-middleHeader\">\r\n        <div class=\"container\">\r\n            <div class=\"middleHeader-item text-left\">\r\n                <div class=\"items item--hamburger\">\r\n                    <a class=\"mobileMenu-toggle\" href=\"#\" data-mobile-menu-toggle=\"menu\">\r\n                        <span class=\"mobileMenu-toggleIcon\">{{lang 'header.toggle_menu'}}</span>\r\n                    </a>\r\n                </div>\r\n                <div class=\"items item--searchMobile\">\r\n                    <a class=\"navUser-action\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\" aria-label=\"{{lang 'search.quick_search.button_search'}}\">\r\n                        <div class=\"navUser-icon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\r\n                        </div>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            <div class=\"middleHeader-item text-center\">\r\n                <div class=\"items item--logo\">\r\n                    {{#if page_type '===' 'default'}}\r\n                        <h1 class=\"header-logo\">\r\n                            {{> components/common/store-logo}}\r\n                        </h1>\r\n                    {{else}}\r\n                        <div class=\"header-logo\">\r\n                            {{> components/common/store-logo}}\r\n                        </div>\r\n                    {{/if}}\r\n                </div>\r\n            </div>\r\n            <div class=\"middleHeader-item text-right\">\r\n                <div class=\"items item--quicksearch halo-search\">\r\n                    <div id=\"quickSearch\" data-prevent-quick-search-close>\r\n                        {{> components/common/quick-search}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"items item--account\">\r\n                    {{#if customer}}\r\n                        <p class=\"myAccount\">{{lang 'account.title'}}</p>\r\n                        <div class=\"myAccount-link\">\r\n                            <a href=\"{{urls.account.index}}\" data-login-form>{{customer.name}}.</a>\r\n                        </div>\r\n                        {{> components/halothemes/halo-account-popup data=\"account-pc\"}}\r\n                    {{else}}\r\n                        <p class=\"myAccount\">{{lang 'account.title'}}</p>\r\n                        <div class=\"myAccount-link\">\r\n                            <a href=\"{{urls.auth.login}}\" data-login-form>\r\n                                {{lang 'common.login'}}\r\n                            </a>\r\n                            <span>or</span>\r\n                            <a href=\"{{urls.auth.create_account}}\">\r\n                                {{lang 'common.sign_up'}}\r\n                            </a>\r\n                        </div>\r\n                        {{> components/halothemes/halo-login-popup data=\"login-pc\" email_id=\"login_email\" password_id=\"login_pass\"}}\r\n                    {{/if}}\r\n                    <a class=\"navUser-action myAccountMobile\" {{#if customer}}href=\"{{urls.account.index}}\"{{else}}href=\"{{urls.auth.login}}\"{{/if}} data-login-form-mobile aria-label=\"{{lang 'account.title'}}\">\r\n                        <div class=\"navUser-icon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-user\"/></svg>\r\n                        </div>\r\n                    </a>\r\n                </div>\r\n                {{#if settings.show_wishlist}}\r\n                    <div class=\"items item--wishlist\">\r\n                        <a class=\"navUser-action\" href=\"{{urls.account.wishlists.all}}\" aria-label=\"{{lang 'common.wish_lists'}}\">\r\n                            <div class=\"navUser-icon\">\r\n                                <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-heart\"></use></svg>\r\n                            </div>\r\n                        </a>\r\n                    </div>\r\n                {{/if}}\r\n                <div class=\"items item--cart halo-cart\">\r\n                    <a class=\"navUser-action cartDesktop\"\r\n                        data-cart-preview\r\n                        data-options=\"align:right\"\r\n                        href=\"{{urls.cart}}\"\r\n                        aria-label=\"{{lang 'cart.nav_aria_label'}}\">\r\n                        <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg><span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                        </div>\r\n                    </a>\r\n                    <div class=\"dropdown-cart\" id=\"cart-preview-dropdown\" aria-hidden=\"true\"></div>\r\n                    <a class=\"navUser-action cartMobile\" data-cart-preview2 href=\"/cart.php\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\r\n                        <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                            <span class=\"countPill cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n                        </div>\r\n                    </a>\r\n                </div>\r\n                <div class=\"nav-language-wrap\">\r\n                    <span></span>\r\n                    <ul class=\"nav-language-selection-wrap\">\r\n                        <li lang=\"fr\">\r\n                            <a href=\"#\" onclick=\"doGTranslate('fr|fr');return false;\" title=\"French\" >                                \r\n                                French\r\n                            </a>                        \r\n                        </li>\r\n                        <li lang=\"en\">\r\n                            <a href=\"#\" onclick=\"doGTranslate('fr|en');return false;\" title=\"English\" >                                \r\n                                English\r\n                            </a>                        \r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"halo-bottomHeader\">\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/common/navigation-menu}}\r\n        </div>\r\n    </div>\r\n</header>\r\n{{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}}\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\" style=\"-webkit-box-orient: vertical;\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/quick-search": "<form class=\"form\" action=\"{{urls.search}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            {{#if theme_settings.halo-search-category}}\r\n                <div id=\"haloSearchCategory\">\r\n                    <label class=\"is-srOnly\" for=\"search_category_query\">{{lang 'header.categories'}}</label>\r\n                    <select name=\"category\" class=\"halo-select-category\" id=\"search_category_query\">\r\n                        <option value=\"\" selected=\"\">{{lang 'header.categories'}}</option>\r\n                        {{#unless page_type '==' 'default'}}\r\n                            {{#each categories}}\r\n                                <option value=\"{{id}}\">{{name}}</option>\r\n                                {{#if children}}\r\n                                    {{#each children}}\r\n                                        <option value=\"{{id}}\">&#8722;&#8722;&#8722; {{name}}</option>\r\n                                        {{#if children}}\r\n                                            {{#each children}}\r\n                                                <option value=\"{{id}}\">&#8722;&#8722;&#8722;&#8722;&#8722;&#8722; {{name}}</option>\r\n                                                {{#if children}}\r\n                                                    {{#each children}}\r\n                                                        {{#if children}}\r\n                                                            <option value=\"{{id}}\">&#8722;&#8722;&#8722;&#8722;&#8722;&#8722;&#8722;&#8722;&#8722; {{name}}</option>\r\n                                                        {{else}}\r\n                                                            <option value=\"{{id}}\">&#8722;&#8722;&#8722;&#8722;&#8722;&#8722;&#8722;&#8722;&#8722; {{name}}</option>\r\n                                                        {{/if}}\r\n                                                    {{/each}}\r\n                                                {{/if}}\r\n                                            {{/each}}\r\n                                        {{/if}}\r\n                                    {{/each}}\r\n                                {{/if}}\r\n                            {{/each}}\r\n                        {{/unless}}\r\n                    </select> \r\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                </div>\r\n            {{/if}}\r\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            <button type=\"submit\" class=\"button button--primary\" aria-label=\"{{lang 'search.quick_search.button_search'}}\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg></button>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n<div class=\"haloQuickSearchResults quickSearchResults\" data-bind=\"html: results\"></div>\r\n{{#if theme_settings.halo-before-quick-results}}\r\n    <div class=\"haloQuickSearchResults quickSearchResultsCustom\">\r\n        {{> components/halothemes/halo-before-quick-results productsnew=products.new}}\r\n    </div>\r\n{{/if}}\r\n{{~inject 'quickSearchBefore' theme_settings.halo-before-quick-results}}\r\n",
  "components/halothemes/halo-account-popup": "<div id=\"{{data}}-popup\" class=\"account-popup {{#if data '==' 'account-pc'}}halo-auth-popup{{/if}}\">\n    <div class=\"halo-popup-content\">\n        <ul class=\"account-dropdown\">\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.orders.all}}\">\n                    <span class=\"text\">{{lang 'account.nav.orders'}}</span>\n                </a>\n            </li>\n            {{#if settings.returns_enabled}}\n                <li class=\"account-item\">\n                    <a class=\"account-action\" href=\"{{urls.account.returns}}\">\n                        <span class=\"text\">{{lang 'account.nav.returns'}}</span>\n                    </a>\n                </li>\n            {{/if}}\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.inbox}}\">\n                    <span class=\"text\">Messages</span>\n                </a>\n            </li>\n             <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.addresses}}\">\n                    <span class=\"text\">{{lang 'account.nav.addresses'}}</span>\n                </a>\n            </li>\n            {{#if settings.show_payment_methods}}\n                <li class=\"account-item\">\n                    <a class=\"account-action\" href=\"{{urls.account.payment_methods.all}}\">\n                        <span class=\"text\">{{lang 'account.nav.payment_methods'}}</span>\n                    </a>\n                </li>\n            {{/if}}\n            {{#if settings.show_wishlist}}\n                <li class=\"account-item\">\n                    <a class=\"account-action\" href=\"{{urls.account.wishlists.all}}\">\n                        <span class=\"text\">Wishlist</span>\n                    </a>\n                </li>\n            {{/if}}\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.recent_items}}\">\n                    <span class=\"text\">{{lang 'account.nav.recently_viewed'}}</span>\n                </a>\n            </li>\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.details}}\">\n                    <span class=\"text\">{{lang 'account.nav.settings'}}</span>\n                </a>\n            </li>\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.auth.logout}}\">\n                    <span class=\"text\">{{lang 'common.logout'}}</span>\n                </a>\n            </li>\n        </ul>\n    </div>\n</div>\n",
  "components/halothemes/halo-login-popup": "<div id=\"{{data}}-popup\" class=\"login-form-popup {{#if data '==' 'login-pc'}}halo-auth-popup{{/if}}\">\n    <div class=\"halo-popup-content\">\n        <form class=\"form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\n            <div class=\"login-form-wrapper\">\n                <div class=\"form-field\">\n                    <label class=\"form-label\" for=\"{{email_id}}\">\n                        {{lang 'login.field_email'}}\n                        <em class=\"text-danger\">*</em>\n                    </label>\n                    <input id=\"{{email_id}}\" class=\"form-input\" type=\"email\" value=\"\" name=\"login_email\" placeholder=\"{{lang 'login.field_email2'}}\">\n                </div>\n                <div class=\"form-field\">\n                    <label class=\"form-label\" for=\"{{password_id}}\">\n                        {{lang 'login.field_password'}}\n                        <em class=\"text-danger\">*</em>\n                    </label>\n                    <input id=\"{{password_id}}\" class=\"form-input\" type=\"password\" value=\"\" placeholder=\"{{lang 'login.field_password'}}\" name=\"login_pass\">\n                </div>\n                <div class=\"form-actions text-center\">\n                    <button type=\"button\" class=\"button button--primary\">{{lang 'login.submit_value' }}</button>\n                    <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password'}}</a>\n                    <a class=\"create-account button\" href=\"{{urls.auth.create_account}}\">{{lang 'login.new_customer.create_account'}}</a>\n                </div>\n            </div>\n        </form>\n    </div>\n</div>\n",
  "components/common/navigation-menu": "<nav class=\"navPages{{#if currency_selector.currencies.length '>' 1}} has-currency{{/if}}\" role=\"navigation\" aria-label=\"Main Navigation\">\n    <ul class=\"navPages-list{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{/if}}\" {{#if theme_settings.haloMenuResize}} id=\"halo-navigation-list\"{{/if}}>\n        {{#each categories}}\n            <li class=\"navPages-item{{#if children}} has-dropdown{{/if}}\">\n                {{> components/common/navigation-list-alternate}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                 <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\n                    {{> components/common/navigation-list-alternate}}\n                 </li>\n            {{/each}}\n        {{/unless}}\n        {{#if theme_settings.haloMenuResize}}\n            <li class=\"navPages-item navPages-item-toggle u-hiddenVisually\">\n                <p class=\"navPages-action has-subMenu is-root\">\n                    <span class=\"text\">\n                        <svg><use xlink:href=\"#icon-more\"></use></svg>\n                    </span>\n                </p>\n                <div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\" id=\"halo-navPages-dropdown\" data-dropdown-menu-content></ul>\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n{{#if currency_selector.currencies.length '>' 1}}\n    <div class=\"navPages-currency\">\n        <ul class=\"navPages-list navPages-list--user\">\n            <li class=\"navPages-item navPages-item-user\">\n                {{> components/common/currency-selector}}\n            </li>\n        </ul>\n    </div>\n{{/if}}\n",
  "components/halothemes/halo-before-quick-results": "<div class=\"quickResults-wrapper\">\n    {{#if theme_settings.halo-before-quick-results-trending-now}}\n        <div class=\"quickResults-item quickResults-text\">\n            {{#if theme_settings.halo-before-quick-results-trending-now-title}}\n                <h3 class=\"quickResults-title\">{{theme_settings.halo-before-quick-results-trending-now-title}}</h3>\n            {{/if}}\n            <ul class=\"productGrid-search\">\n                {{#if theme_settings.halo-before-quick-results-trending-now-1}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-1)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-1}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-2}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-2)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-2}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-3}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-3)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-3}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-4}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-4)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-4}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-5}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-5)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-5}}\n                        </a>\n                    </li>\n                {{/if}}\n            </ul>\n        </div>\n    {{/if}}\n    {{#if theme_settings.halo-before-quick-results-popular}}\n        <div class=\"quickResults-item quickResults-product\">\n            {{#if theme_settings.halo-before-quick-results-popular-title}}\n                <h3 class=\"quickResults-title\">{{theme_settings.halo-before-quick-results-popular-title}}</h3>\n            {{/if}}\n            <div class=\"loadingOverlay\"></div>\n            <ul class=\"productGrid\"></ul>\n        </div>\n    {{/if}}\n</div>\n{{~inject 'quickSearchPopularId' theme_settings.halo-before-quick-results-popular-id}}\n{{~inject 'quickSearchResultLimit' theme_settings.halo-before-quick-results-limit}}\n",
  "components/common/currency-selector": "<a href=\"#\" class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\n    <span class=\"flag\">{{{currency_selector.active_currency_flag}}}</span> \n    <span class=\"text\">\n        {{currency_selector.active_currency_code}}\n    </span>\n    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </span>\n</a>\n<div class=\"navPage-subMenu navPage-subMenu-horizontal\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"text\">\n                    Currency\n                </span>\n            </p>\n        </li>\n        {{#each currency_selector.currencies}}\n            <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} navPages-action-active{{/if}}\" href=\"{{{switch_url}}}\">\n                    <span class=\"flag\">{{{flag}}}</span>\n                    <span class=\"text\">{{code}}</span>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n</div>\n",
  "components/common/navigation-list-alternate": "{{#if children}}\r\n    <p class=\"navPages-action has-subMenu is-root\" data-label=\"{{url}}\">\r\n        {{#if name '==' 'Homme'}}\r\n            <span class=\"text\" style=\"cursor: default;\">{{name}}</span>\r\n        {{else if name '==' 'Femme'}}\r\n            <span class=\"text\" style=\"cursor: default;\">{{name}}</span>\r\n        {{else}}\r\n            <a class=\"text\" href=\"{{url}}\">{{name}}</a>\r\n        {{/if}}\r\n        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n        </span>\r\n    </p>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action navPages-action-end\" data-label=\"{{url}}\" href=\"{{url}}\">\r\n      <span class=\"text\">{{name}}</span>\r\n    </a>\r\n{{/if}}\r\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list navPage-subMenu-list-grid\">\r\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\r\n            <p class=\"navPage-subMenu-action navPages-action\">\r\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                    <span>back</span>\r\n                </span>\r\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\r\n            </p>\r\n        </li>\r\n        {{#if name '==' 'Homme'}}\r\n            \r\n        {{else if name '==' 'Femme'}}\r\n\r\n        {{else}}\r\n            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\r\n                <a class=\"hi navPage-subMenu-action navPages-action\" href=\"{{url}}\">\r\n                    <span class=\"text\">All {{name}}</span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#each children}}\r\n            {{#if children}}\r\n                <li class=\"navPage-subMenu-item-child has-dropdown\">\r\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\r\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\r\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                        </span>\r\n                    </p>\r\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                        <ul class=\"navPage-subMenu-list\">\r\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\r\n                                <p class=\"navPage-subMenu-action navPages-action\">\r\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                        <span>back</span>\r\n                                    </span>\r\n                                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\r\n                                </p>\r\n                            </li>\r\n                            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\r\n                                    <span class=\"text\">All {{name}}</span>\r\n                                </a>\r\n                            </li>\r\n                            {{#each children}}\r\n                                <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\r\n                                </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                    </div>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n"
